#!/usr/bin/env groovy
// see https://jenkins.io/doc/book/pipeline/syntax/

pipeline {

    agent any

    stages {
		
		stage ( '----clean---') {
			steps {
				sh "mvn clean"
			}
		}
		stage('---test--') {
			steps {
				sh "mvn test"
			}
		}
		stage ('----release---') {
			steps {
				sh "git checkout master"
                                sh "git reset --hard origin/master"
				//sh "mvn -B release:rollback"
                                withCredentials([usernamePassword(credentialsId: '', passwordVariable: 'Support5434', usernameVariable: 'glowekouokam')]) {
                                        				sh "mvn -B release:clean release:prepare"
                                }
			}
		}

    }
}
